<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="springboot.service.SearchService">
    <!-- 可根据自己的需求，是否要使用 -->
    <select id="search" resultType="springboot.zz.Doctor" parameterType="int">
        select *
        from doctor
        where id=#{id}
    </select>
</mapper>